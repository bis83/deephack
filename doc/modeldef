;; Gundown Model Specification
;; Edited by BIS

;; Object
(obj objname ...)

;; Use mat
(usemtl matname)

;; Vertex Point
(v (x y z) ...)

;; Normal
(vn (x y z) ...)

;; Tex coord
(vt (u v) ...)

;; Face (a,b,c is a 3-taple of point-index)
(f a b c)

;; Material
(mat matname ...)

(ka r g b)
(kd r g b)
(ks r g b)
(ns x)


